<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relative"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ViewStub
       android:id="@+id/dialog_view_stub"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <include
        android:id="@+id/dialog_frag_baseEmpty"
        layout="@layout/sub_base_empty"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <include
        android:id="@+id/dialog_frag_baseError"
        layout="@layout/sub_base_error"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <include
        android:id="@+id/dialog_frag_baseLoading"
        layout="@layout/sub_base_loading"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</FrameLayout>